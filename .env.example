# Backend Server Port
PORT=3001

# PostgreSQL Database Connection Details
DB_USER=your_pg_user_here
DB_HOST=localhost
DB_DATABASE=photo-tagging-db
DB_PASSWORD=your_pg_password_here
DB_PORT=5432

# Frontend API Base URL (for production deployment)
# In development, Vite/React usually proxies API calls to localhost:PORT
# For production, this will be your deployed backend URL (e.g., https://your-backend-api.render.com)
VITE_API_BASE_URL=http://localhost:3001
# Or if you used REACT_APP_ style for create-react-app based on your initial setup:
# REACT_APP_API_BASE_URL=http://localhost:3001